@article{mars_diffusion-weighted_2011,
	title = {Diffusion-{Weighted} {Imaging} {Tractography}-{Based} {Parcellation} of the {Human} {Parietal} {Cortex} and {Comparison} with {Human} and {Macaque} {Resting}-{State} {Functional} {Connectivity}},
