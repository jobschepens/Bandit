@article{padoa-schioppa_dialogue_2015,
	title = {Dialogue on economic choice, learning theory, and neuronal representations},
	volume = {5},
	issn = {2352-1546},
	url = {http://www.sciencedirect.com/science/article/pii/S2352154615000820},
	doi = {10.1016/j.cobeha.2015.06.004},
	abstract = {In recent years, two distinct lines of work have focused on the substrates of associative learning and on the mechanisms of economic decisions. While experiments often focused on the same brain regions — most notably the orbitofrontal cortex — the two literatures have remained largely distinct. Here we engage in a dialogue with the intent to clarify the relationship between the two frameworks. We identify a potential correspondence between the concept of outcome defined in learning theory and that of good defined in neuroeconomics, and we specifically discuss the concept of value defined in the two frameworks. While many differences remain unresolved, a common idea is that good/outcome values are subjective, devaluation-sensitive and computed on the fly, not ‘cached’ or pre-computed.},
	urldate = {2015-10-21},
	journal = {Current Opinion in Behavioral Sciences},
	author = {Padoa-Schioppa, Camillo and Schoenbaum, Geoffrey},
	month = oct,
