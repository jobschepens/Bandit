	author = {Schad, Daniel J. and Jünger, Elisabeth and Sebold, Miriam and Garbusow, Maria and Bernhardt, Nadine and Javadi, Amir-Homayoun and Zimmermann, Ulrich S. and Smolka, Michael N. and Heinz, Andreas and Rapp, Michael A. and Huys, Quentin J. M.},
	year = {2014},
	keywords = {Cognitive abilities, Decision-making, fluid intelligence, habitual and goal-directed system, model-based and model-free learning, Reward},
