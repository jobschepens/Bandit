	author = {Wulff, Dirk U. and Hills, Thomas T. and Hertwig, Ralph},
	month = jul,
	year = {2015},
	note = {00002},
	keywords = {Amazon, decisions under uncertainty, description–experience gap, information search, online consumer ratings, word of mouth},
	pages = {214--223},
	file = {Full Text PDF:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\K58VGHAH\\Wulff et al. - 2015 - Online Product Reviews and the Description–Experie.pdf:application/pdf;Snapshot:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\B58VMSNW\\abstract.html:text/html}
