@article{eppinger_neuromodulation_2011,
	title = {Neuromodulation of reward-based learning and decision making in human aging},
	volume = {1235},
	copyright = {© 2011 New York Academy of Sciences.},
	issn = {1749-6632},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1749-6632.2011.06230.x/abstract},
	doi = {10.1111/j.1749-6632.2011.06230.x},
	abstract = {In this paper, we review the current literature to highlight relations between age-associated declines in dopaminergic and serotonergic neuromodulation and adult age differences in adaptive goal-directed behavior. Specifically, we focus on evidence suggesting that deficits in neuromodulation contribute to older adults’ behavioral disadvantages in learning and decision making. These deficits are particularly pronounced when reward information is uncertain or the task context requires flexible adaptations to changing stimulus–reward contingencies. Moreover, emerging evidence points to age-related differences in the sensitivity to rewarding and aversive outcomes during learning and decision making if the acquisition of behavior critically depends on outcome processing. These age-related asymmetries in outcome valuation may be explained by age differences in the interplay of dopaminergic and serotonergic neuromodulation. This hypothesis is based on recent neurocomputational and psychopharmacological approaches, which suggest that dopamine and serotonin serve opponent roles in regulating the balance between approach behavior and inhibitory control. Studying adaptive regulation of behavior across the adult life span may shed new light on how the aging brain changes functionally in response to its diminishing resources.},
	language = {en},
	number = {1},
	urldate = {2015-04-27},
	journal = {Annals of the New York Academy of Sciences},
	author = {Eppinger, Ben and Hämmerer, Dorothea and Li, Shu-Chen},
	month = oct,
	year = {2011},
	note = {00057},
	keywords = {aging, cognitive control, motivation, neuromodulation},
	pages = {1--17},
	file = {Eppinger et al_2011_Neuromodulation of reward-based learning and decision making in human aging.pdf:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\NHTGV3DW\\Eppinger et al_2011_Neuromodulation of reward-based learning and decision making in human aging.pdf:application/pdf;Snapshot:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\5S4FK3WJ\\full.html:text/html;Snapshot:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\GDAA8EJB\\abstract.html:text/html}
