@book{link_wave_1992,
	address = {Hillsdale, NJ, England},
	series = {Scientific psychology series.},
	title = {The wave theory of difference and similarity},
	volume = {xv},
	copyright = {(c) 2012 APA, all rights reserved},
	isbn = {0-8058-0926-0 (Hardcover)},
	abstract = {This volume presents and extends the classical psychological theory of judgment. The classical theory originated with Gustav Fechner's extension of Gauss' Theory of Error. Fechner's theory of the comparison of two sensations, and subsequent experimental studies of sensory discrimination and aesthetics, opened to scientific measurement previously unseen mental events. Part I of Wave Theory presents Fechner's theory, the basis for modern statistical theories of choice, and the many scientific achievements of later scientists who tested and advanced Fechner's work. Throughout these advances important roles are played by the three primary measures of discrimination: response frequency, response time, and response confidence. The many seemingly disparate results of Part I are rejoined through a new synthesis presented in Part II. Rather than attributing variability to error, as does the Guassian Theory of Error, variability is created by transceivers that, in response to stimulation, generate a Poisson wave consisting of quantum releases of energy. The comparisons of such Poisson Waves provides the basis for bipolar judgments between stimuli (e.g., more or less and larger or smaller). A separate mechanism compares Poisson Waves for the judgments of equality, of sameness or difference. The analysis of these processes reveals the origins of Weber's Law, Fechner's Law and relates Weber's constants to the exponents of the Power Law advocated by S. S. Stevens. Wave Discrimination Theory gives rise to a theory of sensation and a theory of feeling. Wave Similarity Theory provides a basis for aesthetic judgments and interrelates the various response measures used to investigate feelings of equality.},
	publisher = {Lawrence Erlbaum Associates, Inc},
	author = {Link, Stephen W.},
	year = {1992},
	keywords = {*Judgment, *Perceptual Discrimination, *Stimulus Similarity, Theories},
	file = {APA PsycNET Snapshot:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\7URDDHFK\\1992-97266-000.html:text/html}
}

@article{dai_improved_2015,
	title = {An improved cognitive model of the {Iowa} and {Soochow} {Gambling} {Tasks} with regard to model fitting performance and tests of parameter consistency},
	url = {http://journal.frontiersin.org/article/10.3389/fpsyg.2015.00229/full},
	doi = {10.3389/fpsyg.2015.00229},
	abstract = {The Iowa Gambling Task (IGT) and the Soochow Gambling Task (SGT) are two experience-based risky decision-making tasks for examining decision-making deficits in clinical populations. Several cognitive models, including the expectancy-valence learning (EVL) model and the prospect valence learning (PVL) model, have been developed to disentangle the motivational, cognitive, and response processes underlying the explicit choices in these tasks. The purpose of the current study was to develop an improved model that can fit empirical data better than the EVL and PVL models and, in addition, produce more consistent parameter estimates across the IGT and SGT. Twenty-six opiate users (mean age 34.23; SD 8.79) and 27 control participants (mean age 35; SD 10.44) completed both tasks. Eighteen cognitive models varying in evaluation, updating, and choice rules were fit to individual data and their performances were compared to that of a statistical baseline model to find a best fitting model. The results showed that the model combining the prospect utility function treating gains and losses separately, the decay-reinforcement updating rule, and the trial-independent choice rule performed the best in both tasks. Furthermore, the winning model produced more consistent individual parameter estimates across the two tasks than any of the other models.},
	urldate = {2016-02-08},
	journal = {Decision Neuroscience},
	author = {Dai, Junyi and Kerestes, Rebecca and Upton, Daniel J. and Busemeyer, Jerome R. and Stout, Julie C.},
	year = {2015},
	keywords = {cognitive modeling, Iowa Gambling Task, opiate users, parameter consistency, Soochow Gambling Task},
