	keywords = {Decision-making, Motor planning, optimality, Perceptuo-motor},
	pages = {397--416},
	file = {Jarvstad et al_2014_Are perceptuo-motor decisions really more optimal than cognitive decisions.pdf:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\4RG8UFAR\\Jarvstad et al_2014_Are perceptuo-motor decisions really more optimal than cognitive decisions.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\FC478QKZ\\S0010027713001935.html:text/html}
