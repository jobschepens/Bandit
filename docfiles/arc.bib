	volume = {1},
	shorttitle = {"{Decisions} from experience" = sampling error + prospect theory},
	url = {https://ideas.repec.org/a/jdm/journl/v1y2006ip159-161.html},
	abstract = {According to prospect theory, people overweight low probability events and underweight high probability events. Several recent papers (notably, Hertwig, Barron, Weber {\textbackslash}\& Erev, 2004) have argued that although this pattern holds for ``description-based'' decisions, in which people are explicitly provided with probability distributions over potential outcomes, it is actually reversed in ``experience-based'' decisions, in which people must learn these distributions through sampling. We reanalyze the data of Hertwig et al. (2004) and present a replication to determine the extent to which their phenomenon can be attributed to sampling error (a statistical rather than psychological phenomenon) versus underestimation of rare events (i.e., judgmental bias) versus actual underweighting of judged probabilities. We find that the apparent reversal of prospect theory in decisions from experience can be attributed almost entirely to sampling error, and are consistent with prospect theory and the two-stage model of decision under uncertainty (Fox {\textbackslash}\& Tversky, 1998).},
	urldate = {2015-04-27},
	journal = {Judgment and Decision Making},
	author = {Fox, Craig R. and Hadar, Liat},
	year = {2006},
	note = {00000},
	keywords = {Decisions from experience, decision weights., probability learning, prospect theory, two-stage model, Uncertainty},
	pages = {159--161},
	file = {Fox_Hadar_2006_Decisions from experience = sampling error + prospect theory.pdf:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\U7TNDGM7\\Fox_Hadar_2006_Decisions from experience = sampling error + prospect theory.pdf:application/pdf;RePEc Snapshot:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\7QFXIB23\\v1y2006ip159-161.html:text/html}
