	month = aug,
	year = {2013},
	note = {00066},
	keywords = {Learning and memory, Neurophysiology},
	pages = {575--579},
	file = {Full Text PDF:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\D2SR24QA\\Howe et al. - 2013 - Prolonged dopamine signalling in striatum signals .pdf:application/pdf;Snapshot:C\:\\Users\\schepens\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\js06iwz2.default\\zotero\\storage\\UHE9JD8I\\nature12475.html:text/html}
